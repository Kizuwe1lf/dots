ctrl + shift - c : open -n -a Google\ Chrome
ctrl + shift - s : open -a Spotify
ctrl + shift - g : open -a GoLand
ctrl + shift - v : open -a Visual\ Studio\ Code
ctrl + shift - t : open -a Alacritty
ctrl + shift - p : open -a Slack 
ctrl + shift - f : open -na Finder

# yabai
cmd - 1 : yabai -m space --focus main1
cmd - 2 : yabai -m space --focus main2
cmd - 3 : yabai -m space --focus main3
cmd - 4 : yabai -m space --focus main4
cmd - 5 : yabai -m space --focus main5
cmd - 6 : yabai -m space --focus main6
cmd - 7 : yabai -m space --focus main7
cmd - 8 : yabai -m space --focus main8
cmd - 9 : yabai -m space --focus main9

cmd - 0x32 : yabai -m space --focus recent
cmd - 0x1B : yabai -m space --focus prev
cmd - 0x18 : yabai -m space --focus next

cmd - l : yabai -m window --focus stack.next || yabai -m window --focus east
cmd - h : yabai -m window --focus stack.prev || yabai -m window --focus west

cmd + shift - 1 : yabai -m window --space main1
cmd + shift - 2 : yabai -m window --space main2
cmd + shift - 3 : yabai -m window --space main3
cmd + shift - 4 : yabai -m window --space main4
cmd + shift - 5 : yabai -m window --space main5
cmd + shift - 6 : yabai -m window --space main6
cmd + shift - 7 : yabai -m window --space main7
cmd + shift - 8 : yabai -m window --space main8
cmd + shift - 9 : yabai -m window --space main9
cmd + shift - 0 : yabai -m window --space main0

cmd - m : yabai -m window --focus mouse
cmd - j : yabai -m window --toggle zoom-fullscreen
